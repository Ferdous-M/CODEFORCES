https://codeforces.com/contest/977/problem/C

#include <iostream>
#include<bits/stdc++.h>

using namespace std;

int
main ()
{
  int i, n, j, k = 0, z;
  string s;
  cin >> n >> z;
  int ar[n];
  ar[0] = 1;
  for (i = 1; i <= n; i++)
    {
      cin >> ar[i];
    }
  sort (ar + 1, ar + n + 1);
  if (ar[z] != ar[z + 1])
    cout << ar[z];
  else
    cout << -1;
}
