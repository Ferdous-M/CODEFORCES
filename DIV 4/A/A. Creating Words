https://codeforces.com/contest/1985/problem/A


#include <iostream>
using namespace std;

int main()
{
    int t;cin>>t;
    while(t--){
        string a,b;
        cin>>a>>b;
        cout<<b[0];
        for(int i=1;i<3;i++){
            cout<<a[i];
        }
        cout<<" ";
        cout<<a[0];
        for(int i=1;i<3;i++){
            cout<<b[i];
        }
        cout<<endl;
    }

    return 0;
}
